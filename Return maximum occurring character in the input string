#include<stdio.h>
#include<iostream>
#include <stdlib.h>

using namespace std;
#define MAX_CHAR 265

int* CreateHashmap(int *map, char* str)
{
map=(int*) calloc(MAX_CHAR,sizeof(int));
while(*str)
{
    map[*str]=map[*str]+1;
    str++;
}
return map;
}

int MaxChar(char *str)
{
int *Hash;
Hash=CreateHashmap(Hash,str);


return 1;

}
int main()
{
char str[20]="ayush sharma";
cout<<"Max Char"<<MaxChar(str);
return 0;
}

