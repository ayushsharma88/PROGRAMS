#include <cstdio>
#include <iostream>
#include <list>
#include <string>
#include <vector>
#include <algorithm>

using namespace std;


class HashNode
{
  public:
  int key;
  int value;
  
  HashNode(int Key);
  int getKey();
  int getValue();
  
};

  HashNode::HashNode(int Key)  
  {
      key=Key;
      value=0;
  }
  
  int HashNode::getKey()
  {
      return key;
  }
  
    int HashNode::getValue()
  {
      return value;
  }
  

